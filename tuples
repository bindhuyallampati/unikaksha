# add an item
a=(4,6,2,8,3,1)
print(a)
b=a+(9,)
print(b)
c=a[:5]+(15,20,25)+a[:5]
print(c)
d=c+(30,)
print(d)

# length of a tuple
a=('c','y','f','u','n','o')
print(a)
count=0
for i in a:
    count=count+1
print(count)

# reverse a tuple
a=('c','y','f','u','n','o')
print(a[::-1])

#repeated element in a tuple
c=((2,4,5,6,2,3,4,4,7))
print(c)
n=int(input())
a=c.count(n)
print(a)
