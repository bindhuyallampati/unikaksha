#series 1
n=int(input())
for i in range(1,n+1):
    n=i**2
    print(n,end=' ')

#series 2
n=int(input())
x=6
print(x)
for i in range(1,n):
    x=x+i*5
    print(x,end=' ')
    
    #series 3
    n=int(input())
for i in range(1,n+1):
    n=3**i
    print(n,end=' ')
    
