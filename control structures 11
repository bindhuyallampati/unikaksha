    # series 6
    n=int(input())
for i in range(1,n+1):
    if (i%2!=0):
        print((i**2)-1)
    else:
        print((i**2)-2)
        
        # series 7
        n=int(input())
x=1
print(x)
y=2.0
print(y)
for i in range(1,n-1):
    if(i%2!=0):
        x=x*3.0
        print(x)
    else:
        if(i%2==0):
            y=y*3.0
            print(y)
            
            # series 8
            n=int(input())
a=4
print(a)
for i in range(1,n):
    a=a+(i**2)
