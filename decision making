#greatest of two number
x1=int(input())
y1=int(input())
x2=int(input())
y2=int(input())
mid1=(x1+x2)/2
mid2=(y1+y2)/2
print("Binoy's house is located at",(mid1,mid2))

#vowel or consonants
=input()
if s in "aeiouAEIOU":
    print("vowel")
elif s in "bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ":
    print("consonant")
else:
    print("not an alphabet")
    
    #grading system
    marks=int(input())
if marks==100:
    print("grade S")
elif marks>90 and marks<100:
    print("grade A")
elif marks>=81 and marks<=90:
    print("grade B")
elif marks<=71 and marks<=80:
        print("grade C")
elif marks<=61 and marks<=70:
    print("grade D")
elif marks<=51 and marks<=60:
    print("grade E")
elif marks<=50:
    print("grade F")
else:
    print("Invalid Input")
    
    #profit or loss
    x=float(input("Enter the price of a dozen mangoes\n"))
y=float(input("Enter the price at which 1 mango is being sold\n"))
y=y*12
if x>y:
    print("Loss : Rs.%.2f"%(x-y))
elif x<y:
    print("Profit : Rs.%.2f"%(y-x))
else:
    print("No profit nor loss")
    
    #fee collection
    s=["MSDS","MSH","MGSDS","MGSH"]
print("Enter the student type")
a=str(input())
if a=="MSDS":
    print("Enter tuition fee")
    TF=float(input())
    print("Enter bus fee")
    BF=float(input())
    print("The fees to be paid by the student is Rs.",'%.2f'%(TF+BF),sep='')
elif a=="MSH":
    print("Enter tuition fee")
    TF=float(input())
    print("Enter hostel fee")
    HF=float(input())
    print("The fees to be paid by the student is Rs.",'%.2f'%(TF+HF),sep='')
elif a=="MGSDS":
    print("Enter tuition fee")  
    TF=float(input())
    print("Enter bus fee")
    BF=float(input())
    print("The fees to be paid by the student is Rs.",'%.2f'%((1.5*TF)+BF),sep='')
else:
    print("Enter tuition fee")
    TF=float(input())
    print("Enter hostel fee")
    HF=float(input())
    print("The fees to be paid by the student is Rs.",'%.2f'%((1.5*TF)+HF),sep='')

#y2k problem detector
print("enter year of Birth")
Birth=int(input())
print("enter Current year")
Current=int(input())
if Current<Birth:
    x=100-Birth
    y=x+Current
    print("your age is ",y)
else:
    print("your age is",Current-Birth)
    
