    #gardening 1
    r=int(input())
c=int(input())
t=int(input())
if(t<=r*c):
  if(t<=c):
    print("Yes")
  elif(t%c==0):
    print("Yes")
  elif((t-1)%c==0):
    print("yes")
  else:
      print("NO")
else:
    print("No")
        
#hotel tarift calculator
m=(input())
r=float(input())
d=int(input())
tr=((r*20/100)+r)*d
if m in "4561112":
    print("Hotel Tariff:Rs.%.2f"%tr)
elif m in "12378910":
    print("Hotel Tariff:Rs.%.2f"%(r*d))
else:
    print("invalid input")
    
